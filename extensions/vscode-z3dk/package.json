{
  "name": "z3dk",
  "displayName": "Z3DK Zelda Hacking Toolkit",
  "description": "Asar syntax, z3lsp language server, and Zelda 3 tooling helpers.",
  "version": "0.1.0",
  "publisher": "scawful",
  "engines": {
    "vscode": ">=1.85.0"
  },
  "categories": ["Programming Languages", "Other"],
  "activationEvents": [
    "*",
    "onStartupFinished",
    "onLanguage:asar",
    "onView:z3dk.commands",
    "onView:z3dk.dashboard",
    "onView:z3dk.labels",
    "onView:z3dk.romMap",
    "onCommand:z3dk.openRomMap",
    "onCommand:z3dk.runTests",
    "onCommand:z3dk.build",
    "onCommand:z3dk.exportSymbols",
    "onCommand:z3dk.restartServer",
    "onCommand:z3dk.openWorkspace",
    "onCommand:z3dk.selectProject",
    "onCommand:z3dk.openMainAsm",
    "onCommand:z3dk.selectRom",
    "onCommand:z3dk.openReadme",
    "onCommand:z3dk.openModelCatalog",
    "onCommand:z3dk.openModelPortfolio",
    "onCommand:z3dk.openContinueConfig",
    "onCommand:z3dk.openContinueConfigTs",
    "onCommand:z3dk.openAfsScratchpad",
    "onCommand:z3dk.addAfsContexts",
    "onCommand:z3dk.openOracleRepo",
    "onCommand:z3dk.openYazeRepo",
    "onCommand:z3dk.openMesenRepo",
    "onCommand:z3dk.openDashboard",
    "onCommand:z3dk.launchMesen",
    "onCommand:z3dk.launchYaze",
    "onCommand:z3dk.openRomFolder",
    "onCommand:z3dk.openUsdasmRoot",
    "onCommand:z3dk.findUsdasmLabel",
    "onCommand:z3dk.exportDisassembly",
    "onCommand:z3dk.openDisassemblyOutput",
    "onCommand:z3dk.openDisassemblyFile",
    "onCommand:z3dk.openLatestDisassemblyFile",
    "onCommand:z3dk.openYazeLog",
    "onCommand:z3dk.findLabel",
    "onCommand:z3dk.openLabelSearch",
    "onCommand:z3dk.generateLabelIndex",
    "onCommand:z3dk.lspStatusOn",
    "onCommand:z3dk.lspStatusOff",
    "onCommand:z3dk.refresh",
    "onCommand:z3dk.refreshDashboard",
    "onLanguage:65816-assembly"
  ],
  "main": "extension.js",
  "contributes": {
    "languages": [
      {
        "id": "asar",
        "aliases": ["Asar", "z3asm", "65816-assembly"],
        "extensions": [".asm", ".inc", ".s", ".a65", ".zasm"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "asar",
        "scopeName": "source.asar",
        "path": "./syntax/asar.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "asar",
        "path": "./snippets/asar.json"
      }
    ],
    "commands": [
      {
        "command": "z3dk.openWorkspace",
        "title": "Z3DK: Workspace"
      },
      {
        "command": "z3dk.selectProject",
        "title": "Z3DK: Select Project"
      },
      {
        "command": "z3dk.openMainAsm",
        "title": "Z3DK: Main ASM"
      },
      {
        "command": "z3dk.selectRom",
        "title": "Z3DK: Select ROM"
      },
      {
        "command": "z3dk.openReadme",
        "title": "Z3DK: README"
      },
      {
        "command": "z3dk.openModelCatalog",
        "title": "Z3DK: Model Catalog"
      },
      {
        "command": "z3dk.openModelPortfolio",
        "title": "Z3DK: Model Portfolio"
      },
      {
        "command": "z3dk.openContinueConfig",
        "title": "Z3DK: Continue YAML"
      },
      {
        "command": "z3dk.openContinueConfigTs",
        "title": "Z3DK: Continue TS"
      },
      {
        "command": "z3dk.openAfsScratchpad",
        "title": "Z3DK: AFS Scratchpad"
      },
      {
        "command": "z3dk.addAfsContexts",
        "title": "Z3DK: Add AFS Contexts"
      },
      {
        "command": "z3dk.runTests",
        "title": "Z3DK: Tests"
      },
      {
        "command": "z3dk.build",
        "title": "Z3DK: Build"
      },
      {
        "command": "z3dk.exportSymbols",
        "title": "Z3DK: Symbols"
      },
      {
        "command": "z3dk.restartServer",
        "title": "Z3DK: Restart LSP"
      },
      {
        "command": "z3dk.openDashboard",
        "title": "Z3DK: Dashboard"
      },
      {
        "command": "z3dk.openOracleRepo",
        "title": "Z3DK: Oracle Repo"
      },
      {
        "command": "z3dk.openYazeRepo",
        "title": "Z3DK: Yaze Repo"
      },
      {
        "command": "z3dk.openMesenRepo",
        "title": "Z3DK: Mesen2 Repo"
      },
      {
        "command": "z3dk.launchMesen",
        "title": "Z3DK: Mesen2"
      },
      {
        "command": "z3dk.launchYaze",
        "title": "Z3DK: yaze"
      },
      {
        "command": "z3dk.openRomFolder",
        "title": "Z3DK: ROM Folder"
      },
      {
        "command": "z3dk.openUsdasmRoot",
        "title": "Z3DK: USDASM Root"
      },
      {
        "command": "z3dk.openLabelSearch",
        "title": "Z3DK: Label Search Panel"
      },
      {
        "command": "z3dk.findLabel",
        "title": "Z3DK: Label Search"
      },
      {
        "command": "z3dk.generateLabelIndex",
        "title": "Z3DK: Regenerate Label Index"
      },
      {
        "command": "z3dk.findUsdasmLabel",
        "title": "Z3DK: USDASM Search"
      },
      {
        "command": "z3dk.exportDisassembly",
        "title": "Z3DK: Hack Disasm"
      },
      {
        "command": "z3dk.openDisassemblyOutput",
        "title": "Z3DK: Disasm Output"
      },
      {
        "command": "z3dk.openDisassemblyFile",
        "title": "Z3DK: Disasm File"
      },
      {
        "command": "z3dk.openLatestDisassemblyFile",
        "title": "Z3DK: Latest Disasm"
      },
      {
        "command": "z3dk.openYazeLog",
        "title": "Z3DK: Yaze Log"
      },
      {
        "command": "z3dk.lspStatusOn",
        "title": "Z3DK LSP: on"
      },
      {
        "command": "z3dk.lspStatusOff",
        "title": "Z3DK LSP: off"
      },
      {
        "command": "z3dk.initProject",
        "title": "Z3DK: New Zelda Project"
      },
      {
        "command": "z3dk.openRomMap",
        "title": "Z3DK: Open ROM Map"
      },
      {
        "command": "z3dk.refresh",
        "title": "Z3DK: Refresh Sidebar"
      },
      {
        "command": "z3dk.refreshDashboard",
        "title": "Z3DK: Refresh Dashboard"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "z3dk",
          "title": "Z3DK",
          "icon": "assets/triforce.svg"
        }
      ]
    },
    "views": {
      "z3dk": [
        {
          "id": "z3dk.dashboard",
          "name": "Dashboard",
          "type": "webview"
        },
        {
          "id": "z3dk.labels",
          "name": "Labels",
          "type": "webview"
        },
        {
          "id": "z3dk.romMap",
          "name": "ROM Map",
          "type": "webview"
        },
        {
          "id": "z3dk.commands",
          "name": "Commands"
        }
      ]
    },
    "submenus": [
      {
        "id": "z3dk.menu",
        "label": "Z3DK"
      }
    ],
    "menus": {
      "menubar/view": [
        {
          "submenu": "z3dk.menu",
          "group": "9_z3dk"
        }
      ],
      "z3dk.menu": [
        {
          "command": "z3dk.openWorkspace",
          "group": "1_general"
        },
        {
          "command": "z3dk.openReadme",
          "group": "1_general"
        },
        {
          "command": "z3dk.openDashboard",
          "group": "1_general"
        },
        {
          "command": "z3dk.openLabelSearch",
          "group": "1_general"
        },
        {
          "command": "z3dk.openModelCatalog",
          "group": "2_resources"
        },
        {
          "command": "z3dk.openModelPortfolio",
          "group": "2_resources"
        },
        {
          "command": "z3dk.openContinueConfig",
          "group": "2_resources"
        },
        {
          "command": "z3dk.openContinueConfigTs",
          "group": "2_resources"
        },
        {
          "command": "z3dk.addAfsContexts",
          "group": "2_resources"
        },
        {
          "command": "z3dk.openRomFolder",
          "group": "3_emulator"
        },
        {
          "command": "z3dk.launchMesen",
          "group": "3_emulator"
        },
        {
          "command": "z3dk.launchYaze",
          "group": "3_emulator"
        },
        {
          "command": "z3dk.exportDisassembly",
          "group": "4_disasm"
        },
        {
          "command": "z3dk.openLatestDisassemblyFile",
          "group": "4_disasm"
        },
        {
          "command": "z3dk.openDisassemblyOutput",
          "group": "4_disasm"
        },
        {
          "command": "z3dk.openDisassemblyFile",
          "group": "4_disasm"
        },
        {
          "command": "z3dk.findUsdasmLabel",
          "group": "4_disasm"
        },
        {
          "command": "z3dk.findLabel",
          "group": "4_disasm"
        },
        {
          "command": "z3dk.openUsdasmRoot",
          "group": "4_disasm"
        },
        {
          "command": "z3dk.generateLabelIndex",
          "group": "4_disasm"
        },
        {
          "command": "z3dk.build",
          "group": "5_actions"
        },
        {
          "command": "z3dk.runTests",
          "group": "5_actions"
        },
        {
          "command": "z3dk.exportSymbols",
          "group": "5_actions"
        },
        {
          "command": "z3dk.restartServer",
          "group": "6_server"
        },
        {
          "command": "z3dk.openOracleRepo",
          "group": "7_repos"
        },
        {
          "command": "z3dk.openYazeRepo",
          "group": "7_repos"
        },
        {
          "command": "z3dk.openMesenRepo",
          "group": "7_repos"
        }
      ],
      "editor/context": [
        {
          "command": "z3dk.openDashboard",
          "when": "z3dk.isAsmLanguage",
          "group": "z3dk@1"
        },
        {
          "command": "z3dk.exportSymbols",
          "when": "z3dk.isAsmLanguage && z3dk.lspActive",
          "group": "z3dk@2"
        },
        {
          "command": "z3dk.findLabel",
          "when": "z3dk.isAsmLanguage && z3dk.lspActive",
          "group": "z3dk@2"
        },
        {
          "command": "z3dk.findUsdasmLabel",
          "when": "z3dk.isAsmLanguage && z3dk.lspActive",
          "group": "z3dk@2"
        },
        {
          "command": "z3dk.exportDisassembly",
          "when": "z3dk.isAsmLanguage && z3dk.lspActive",
          "group": "z3dk@2"
        },
        {
          "command": "z3dk.openDisassemblyOutput",
          "when": "z3dk.isAsmLanguage",
          "group": "z3dk@2"
        },
        {
          "command": "z3dk.openLatestDisassemblyFile",
          "when": "z3dk.isAsmLanguage",
          "group": "z3dk@2"
        },
        {
          "command": "z3dk.restartServer",
          "when": "z3dk.isAsmLanguage",
          "group": "z3dk@3"
        }
      ],
      "editor/title": [
        {
          "command": "z3dk.lspStatusOn",
          "when": "z3dk.isAsmLanguage && z3dk.lspActive",
          "group": "navigation@1"
        },
        {
          "command": "z3dk.lspStatusOff",
          "when": "z3dk.isAsmLanguage && !z3dk.lspActive",
          "group": "navigation@1"
        }
      ],
      "view/title": [
        {
          "command": "z3dk.refresh",
          "when": "view == z3dk.commands",
          "group": "navigation"
        },
        {
          "command": "z3dk.refreshDashboard",
          "when": "view == z3dk.dashboard",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Z3DK",
      "properties": {
        "z3dk.devWorkspacePath": {
          "type": "string",
          "default": "",
          "description": "Path to the multi-root dev workspace (.code-workspace)."
        },
        "z3dk.modelCatalogPath": {
          "type": "string",
          "default": "",
          "description": "Path to MODEL_CATALOG.md for Zelda model references."
        },
        "z3dk.modelPortfolioPath": {
          "type": "string",
          "default": "",
          "description": "Path to MODEL_PORTFOLIO.md for AFS model references."
        },
        "z3dk.continueConfigPath": {
          "type": "string",
          "default": "",
          "description": "Path to Continue config.yaml."
        },
        "z3dk.continueConfigTsPath": {
          "type": "string",
          "default": "",
          "description": "Path to Continue config.ts."
        },
        "z3dk.oracleRoot": {
          "type": "string",
          "default": "",
          "description": "Path to oracle-of-secrets repo (used for integrations)."
        },
        "z3dk.yazeRoot": {
          "type": "string",
          "default": "",
          "description": "Path to yaze repo (used for integrations)."
        },
        "z3dk.mesenRoot": {
          "type": "string",
          "default": "",
          "description": "Path to mesen2-oos repo (used for integrations)."
        },
        "z3dk.mesenPath": {
          "type": "string",
          "default": "",
          "description": "Path to the Mesen2-OOS executable or .app bundle."
        },
        "z3dk.mesenArgs": {
          "type": "array",
          "default": [],
          "items": {"type": "string"},
          "description": "Arguments for launching Mesen2-OOS (supports ${rom} and ${symbols})."
        },
        "z3dk.yazeLaunchArgs": {
          "type": "array",
          "default": [],
          "items": {"type": "string"},
          "description": "Arguments for launching yaze (supports ${rom} and ${symbols})."
        },
        "z3dk.usdasmRoot": {
          "type": "string",
          "default": "",
          "description": "Path to the USDASM source-of-truth disassembly."
        },
        "z3dk.usdasmGlob": {
          "type": "string",
          "default": "**/*.asm,**/*.inc,**/*.s,**/*.a65",
          "description": "Glob for searching USDASM labels."
        },
        "z3dk.labelIndexPath": {
          "type": "string",
          "default": "",
          "description": "Path to the combined label index CSV (label_index_all.csv). Leave empty to auto-detect."
        },
        "z3dk.labelMapPath": {
          "type": "string",
          "default": "",
          "description": "Path to merged label map CSV (labels_merged.csv). Leave empty to auto-detect."
        },
        "z3dk.labelIndexCommand": {
          "type": "string",
          "default": "",
          "description": "Command to regenerate label indexes (supports ${workspaceFolder} / ${workspaceRoot} / ${z3dk})."
        },
        "z3dk.autoGenerateLabelIndex": {
          "type": "boolean",
          "default": true,
          "description": "Auto-regenerate label indexes after build/disasm/symbol export."
        },
        "z3dk.labelIndexDebounceMs": {
          "type": "number",
          "default": 8000,
          "description": "Debounce delay before regenerating label indexes."
        },
        "z3dk.labelSearchLimit": {
          "type": "number",
          "default": 200,
          "description": "Maximum number of label search results to show."
        },
        "z3dk.disasmCommand": {
          "type": "string",
          "default": "",
          "description": "Command to export a USDASM-style disassembly (supports ${rom}, ${symbols}, ${output}, ${usdasm}, ${labels})."
        },
        "z3dk.disasmOutputPath": {
          "type": "string",
          "default": "",
          "description": "Output path for exported disassembly artifacts."
        },
        "z3dk.autoOpenDisassemblyOutput": {
          "type": "boolean",
          "default": false,
          "description": "Auto-open disassembly output after running Hack Disasm."
        },
        "z3dk.languageIds": {
          "type": "array",
          "default": ["asar", "asm", "65816", "65c816", "wla-65816", "snes-asm", "snesasm", "super-nes-asm"],
          "items": {"type": "string"},
          "description": "Language ids that should activate the Z3DK LSP."
        },
        "z3dk.autoStart": {
          "type": "boolean",
          "default": true,
          "description": "Auto-start z3lsp when opening Asar/65816 files."
        },
        "z3dk.serverPath": {
          "type": "string",
          "default": "",
          "description": "Path to the z3lsp binary. Leave empty to auto-detect (build/*/src/z3lsp/z3lsp or PATH)."
        },
        "z3dk.serverArgs": {
          "type": "array",
          "default": [],
          "items": {"type": "string"},
          "description": "Additional arguments to pass to z3lsp."
        },
        "z3dk.buildCommand": {
          "type": "string",
          "default": "cmake --build build",
          "description": "Command to build z3dk tools."
        },
        "z3dk.testCommand": {
          "type": "string",
          "default": "./run_tests.sh",
          "description": "Command to run z3dk regression tests."
        },
        "z3dk.yazePath": {
          "type": "string",
          "default": "yaze",
          "description": "Path to the yaze executable used for symbol export."
        },
        "z3dk.yazeQuiet": {
          "type": "boolean",
          "default": true,
          "description": "Redirect yaze stdout/stderr to a log file to reduce terminal spam."
        },
        "z3dk.yazeLogPath": {
          "type": "string",
          "default": "",
          "description": "Path to write yaze logs (defaults to ~/.cache/z3dk/yaze.log)."
        },
        "z3dk.romPath": {
          "type": "string",
          "default": "",
          "description": "ROM path to use for symbol export (required for export command)."
        },
        "z3dk.projectRoot": {
          "type": "string",
          "default": "",
          "description": "Override Z3DK project root (folder containing z3dk.toml)."
        },
        "z3dk.symbolFormat": {
          "type": "string",
          "default": "mesen",
          "description": "Symbol format to export (mesen, wla, nocash, etc)."
        },
        "z3dk.symbolsPath": {
          "type": "string",
          "default": "",
          "description": "Override symbol output path. Defaults to <rom>.mlb for mesen and <rom>.sym otherwise."
        }
      }
    }
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
